body {
	background-color: #111;
	font-size: 20px;
	margin-top: 30px;
}

a {
	color: #FFF;
}

a:hover, a:focus, a:active {
	color: #FFF;
}

a:focus, a:active {
	text-decoration: none;
}

li {
	color: #BBB;
	list-style: none;
	margin: 20px 0;
	line-height: 30px;
}

.fe-meta {
	font-size: 14px;
	color: #666;
}

.fe-btn-remove {
	color: #666;
	padding-top: 0;
	padding-bottom: 0;
	vertical-align: inherit;
}

.fe-btn-remove:active, .fe-btn-remove:focus {
	outline-color: #111 !important;
	color: #666;
}

.fe-btn-remove:hover {
	color: #FFF;
}

.fe-done {
	color: #FFF;
}
